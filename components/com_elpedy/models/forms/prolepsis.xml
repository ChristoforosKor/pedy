<form>
    
    <fields>
        <fieldset>
            <field 
                name="id" 
                type="hidden"
                        
            />
                    
            <field 
                label="JDATE"
                name="RefDate" 
                type="Calendar"
                required="true"
                format="%d/%m/%Y"
                default="NOW" 
                class="calendar-form-control"
            >
               
            </field>
            <field 
                label="COM_EL_PEDY_EXAMS_CENTER"
                name="exam_center_id" 
                type="SQLRemote"
                query="select id as value, exam_center as exam_center_id from ExamCenter order by exam_center"
                required="true" 
                class="form-control"
            >
                <option value="" >JGLOBAL_SELECT_AN_OPTION</option>
            </field>
            
            
            <field 
                label="COM_EL_PEDY_VIALS_RECEIVED_UNIT_HC"
                name="vials_received" 
                type="number"
                required="true" 
                min="0"
                class="form-control"
            >
            </field>
            <field 
                label="COM_EL_PEDY_SAMPLES_TO_HC"
                name="samples_to_hc" 
                type="number"
                required="true"
                min="0" 
                class="form-control"
            >
            </field>
            <field 
                label="COM_EL_PEDY_NEGATIVE"
                name="result_negative" 
                type="number"
                min="0"
                class="form-control"
            >
            </field>
            <field 
                label="COM_EL_PEDY_HPV_16"
                name="result_positive_hpv16" 
                type="number"
                min="0"
                class="form-control"
            >
            </field>
            <field 
                label="COM_EL_PEDY_HPV_18"
                name="result_positive_hpv18" 
                type="number"
                min="0"
                class="form-control"
            >
            </field>
            <field 
                label="COM_EL_PEDY_POSITIVE_TO_ASCSUS"
                name="result_positive_ascsus" 
                type="number"
                min="0"    
                class="form-control"                 
            >
            </field>
            <field 
                label="COM_EL_PEDY_NEGATIVE"
                name="result_positive_to_pap_negative" 
                type="number"
                min="0"  
                class="form-control"                   
            >
            </field>
            <field 
                label="COM_EL_PEDY_VIALS_STOCK"
                name="vials_in_stock" 
                type="number"
                min="0"    
                class="form-control "                 
            >
            </field>
                    
        </fieldset>
    </fields>
</form>
